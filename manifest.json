{
    "name": "Bears Extensions",
    "description" : "https://www.youtube.com/watch?v=Ipa58NVGs_c&t=0s",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_title": "Bears"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": ["tabs"]
}